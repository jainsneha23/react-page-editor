{
  "name": "page-editor",
  "version": "1.0.0",
  "description": "Drag and drop to create a page in REACT",
  "main": "tools/server.js",
  "scripts": {
    "test": "test",
    "devstart": "npm run lint & npm run devserver",
    "devserver": "./node_modules/.bin/babel-node tools/devserver.js",
    "lint": "node_modules/.bin/eslint src tools scripts",
    "lint:watch": "npm run lint -- --watch",
    "build": "BABEL_DISABLE_CACHE=1 NODE_ENV=production ./node_modules/.bin/babel-node ./node_modules/.bin/webpack --config tools/webpack.config.prod.js",
    "server": "BABEL_DISABLE_CACHE=1 NODE_ENV=production node index.js &",
    "start": "npm run build & npm run server"
  },
  "dependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-register": "^6.16.3",
    "cluster": "^0.7.7",
    "colors": "^1.1.2",
    "compression": "^1.6.2",
    "css-loader": "^0.25.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "minimist": "^1.2.0",
    "os": "^0.1.1",
    "reset-css": "^2.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "eslint": "^3.5.0",
    "eslint-plugin-react": "^6.2.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "webpack-dev-middleware": "^1.7.0",
    "webpack-hot-middleware": "^2.12.2"
  }
}
